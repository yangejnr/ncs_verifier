{
  "expo": {
    "name": "NCS Verify",
    "slug": "ncs-verify",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/branding/ncs_logo.png",
    "splash": {
      "image": "./assets/branding/ncs_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B1F2A"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/ncs_logo.png",
        "backgroundColor": "#0B1F2A"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.yangejnr.ncsverify"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-camera"
    ],
    "extra": {
      "gatewayUrl": "http://127.0.0.1:7001"
    },
    "ios": {
      "bundleIdentifier": "com.yangejnr.ncsverify"
    }
  }
}
